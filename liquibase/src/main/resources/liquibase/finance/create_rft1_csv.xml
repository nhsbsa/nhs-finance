<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet author="mhood" id="create-rft1-csv-table">
        <createTable schemaName="public" tableName="csv_data">
            <column autoIncrement="true" name="csv_file_id" type="BIGINT">
                <constraints primaryKey="true" primaryKeyName="csv_file_id"/>
            </column>
            <column name="account_code" type="VARCHAR(255)"/>
            <column name="transaction_date" type="VARCHAR(255)"/>
            <column name="base_amount" type="VARCHAR(255)"/>
            <column name="base_amount_plus_minus" type="VARCHAR(255)"/>
            <column name="payment_type" type="VARCHAR(255)"/>
            <column name="reference" type="VARCHAR(255)"/>
            <column name="description" type="VARCHAR(255)"/>
            <column name="sales_analysis" type="VARCHAR(255)"/>
            <column name="journal_source" type="VARCHAR(255)"/>
            <column name="contribution_month" type="VARCHAR(255)"/>
            <column name="staff_gp" type="VARCHAR(255)"/>
            <column name="contribution-type" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
